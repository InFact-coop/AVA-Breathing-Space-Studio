{
  "name": "avabreathingspace",
  "private": true,
  "version": "1.0.0",
  "description": "_SEO friendly page builder in React.js. Heroes, sign-up forms and calls to action._",
  "main": "package.json",
  "author": "Max Gerber <mgerber94@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity start",
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "sanity graphql deploy && sanity deploy",
    "build": "sanity build public -y",
    "==lint&format==": "-----------------------------------------------------------------------------------",
    "format": "npm run prettier -- --write",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "keywords": [
    "sanity"
  ],
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true,
      "ext": [
        ".js",
        ".ts"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@sanity/base": "^1.150.7",
    "@sanity/components": "^1.150.7",
    "@sanity/core": "^1.150.7",
    "@sanity/default-layout": "^1.150.7",
    "@sanity/default-login": "^1.150.1",
    "@sanity/desk-tool": "^1.150.7",
    "@sanity/production-preview": "^1.150.1",
    "@sanity/vision": "^2.1.4",
    "prop-types": "^15.6",
    "react": "^16.2",
    "react-dom": "^16.2",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@sanity/cli": "^1.150.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-kentcdodds": "^14.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.2.1",
    "shell-quote": "^1.7.2",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maxgerber/AVA-Breathing-Space.git"
  }
}
